import re

def cek_item(item, daftar_belanja):
    if item in daftar_belanja:
        return True
    else:
        return False

def tampilkan_daftar_belanja(daftar_belanja):
    print("Daftar belanja saat ini:")
    for item, kuantitas in daftar_belanja.items():
        print(f"- {kuantitas}Â {item}")
